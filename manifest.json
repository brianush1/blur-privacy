{
	"manifest_version": 2,
	"name": "Blur privacy",
	"description": "Prevents websites from knowing when they lose your attention",
	"version": "1.0",
	"content_scripts": [
		{
			"js": ["privacy.js"],
			"matches": ["<all_urls>"],
			"match_about_blank": true,
			"all_frames": true,
			"run_at": "document_start"
		}
	]
}